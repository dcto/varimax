{
  "name": "varimax/varimax",
  "description": "Varimax The Full Stack PHP Frameworks",
  "type": "project",
  "keywords": ["php","php framework", "varimax-framework"],
  "license": "MIT",
  "prefer-stable" : true,
  "minimum-stability": "dev",
  "homepage": "http://www.varimax.cn",
  "support": {
    "issues": "https://github.com/dcto/varimax/issues",
    "source": "https://github.com/dcto/varimax"
  },
  "authors": [
    {
      "name": "Sem.dc",
      "email": "sdoz@live.com"
    }
  ],

  "require": {
    "php": "^7.2.0",
    "ext-xml":"*",
    "ext-json":"*",
    "ext-curl":"*",
    "ext-bcmath": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "psr/log": "1.*",
    "symfony/http-foundation": "3.*",
    "illuminate/contracts": "6.20.*",
    "illuminate/container": "6.20.*",
    "illuminate/support": "6.20.*",
    "illuminate/events": "6.20.*",
    "illuminate/database": "6.20.*",
    "illuminate/filesystem": "6.20.*",
    "illuminate/pagination": "6.20.*",
    "twig/twig": "1.*",
    "vlucas/phpdotenv":"^3.3",
    "hashids/hashids": "^4.0",
    "nesbot/carbon": "^2.50.0" 
  },
  "autoload": {
    "psr-4": {
      "VM\\": "src/"
    },
    "files": [
      "src/Bootstrap.php"
    ],
    "classmap":[
      "src/Support/Arr.php",
      "src/Support/Str.php",
      "src/Support/Facades"
    ]
  }
}
