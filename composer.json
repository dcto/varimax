{
  "name": "varimax/varimax",
  "description": "Varimax The Slim PHP Frameworks",
  "type": "project",
  "keywords": ["php","php framework", "varimax-framework"],
  "license": "MIT",
  "prefer-stable" : true,
  "minimum-stability": "dev",
  "homepage": "http://www.varimax.cn",
  "support": {
    "issues": "https://github.com/dcto/varimax/issues",
    "source": "https://github.com/dcto/varimax"
  },
  "authors": [
    {
      "name": "dc.To",
      "email": "sdoz@live.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": "^7.3",
    "ext-xml":"*",
    "ext-json":"*",
    "ext-curl":"*",
    "ext-bcmath": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "psr/log": "1.*",
    "vlucas/phpdotenv":"3.3.*",
    "illuminate/contracts": "8.*",
    "illuminate/container": "8.*",
    "illuminate/database": "8.*",
    "illuminate/events": "8.*",
    "illuminate/filesystem": "8.*",
    "illuminate/http": "8.*",
    "illuminate/support": "8.*",            
    "illuminate/pagination": "8.*"
  },
  "suggest": {    
    "hashids/hashids": "Suggest HashID In the Eloquent",
    "nesbot/carbon": "Suggest Carbon DateTime Formating",
    "windwalker/renderer": "Suggest 3.* Renderer View Engine ",
    "twig/twig": "Suggest Twig 2.x Of View Engine",
    "league/plates": "Suggest Plates 3.x Of View Engine",
    "windwalker/edge": "Suggest Edge 3.x Of View Engine",
    "illuminate/view": "Suggest Blade 7.* Of View Engine",
    "mustache/mustache": "Suggest Mustcache 2.x Of View Engine"
  },
  "autoload": {
    "psr-4": {
      "VM\\": "src"
    },
    "classmap":[
      "src/Facades"
    ],
    "files": ["helper.php","vrimax.php"]
  }
}
