{
  "name": "varimax/varimax",
  "description": "Varimax The Slim PHP Frameworks",
  "type": "project",
  "keywords": ["php","php framework", "varimax-framework"],
  "license": "MIT",
  "prefer-stable" : true,
  "minimum-stability": "dev",
  "homepage": "http://www.varimax.cn",
  "support": {
    "issues": "https://github.com/dcto/varimax/issues",
    "source": "https://github.com/dcto/varimax"
  },
  "authors": [
    {
      "name": "dc.To",
      "email": "sdoz@live.com"
    }
  ],
  "autoload": {
    "classmap":["src/Facades"],
    "files": ["bin/varimax.php"],
    "psr-4": {"VM\\": "src"}
  },
  "bin": [
    "bin/varimax.php",
    "bin/server.php"
  ],
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "php": "^7.3|^8.0",
    "ext-xml":"*",
    "ext-json":"*",
    "ext-curl":"*",
    "ext-bcmath": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "psr/log": "1.*",
    "psr/http-message": "1.*",
    "vlucas/phpdotenv":"5.*",
    "illuminate/contracts": "8.*",
    "illuminate/container": "8.*",
    "illuminate/database": "8.*",
    "illuminate/events": "8.*",
    "illuminate/filesystem": "8.*",
    "illuminate/http": "8.*",
    "illuminate/support": "8.*",  
    "illuminate/pipeline": "8.*",          
    "illuminate/pagination": "8.*"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Suggest Http client of 6.*",
    "nesbot/carbon": "Suggest Carbon DateTime Formating",
    "varimax/view":"Suggest varimax template engine"
  }
}
